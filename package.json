{
  "name": "sdfwa-digital-services",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "db:migrate": "turbo run db:migrate",
    "db:generate": "turbo run db:generate",
    "start:dev": "node scripts/dev.js",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/react": "^19.2.13",
    "@sdfwa/eslint-config": "workspace:*",
    "@sdfwa/typescript-config": "workspace:*",
    "prettier": "^3.8.1",
    "turbo": "^2.8.1",
    "typescript": "5.9.3"
  },
  "packageManager": "bun@1.3.8",
  "engines": {
    "node": ">=24"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}